        "subnetName": {
            "value": null
        },
        "ipAddresses": {
            "value": null
        },
        "vmExtensionName": {
            "value": "ChefExtension"
        },
        "chef_client": {
            "value": "ChefClient"
        },
        "chef_node_environment": {
            "value": "_default"
        },
