        "location": {
            "value": "eastus2"
        },
        "virtualMachineSize": {
            "value": "Standard_F1s"
        },
        "adminUsername": {
            "value": "admin"
        },
        "virtualNetworkRG": {
            "value": null
        },
        "virtualNetworkName": {
            "value": null
        },
        "adminPassword": {
            "value": null
        },
        "storageAccountType": {
            "value": "Premium_LRS"
        },
        "diagnosticsStorageAccountType": {
            "value": "Standard_LRS"
